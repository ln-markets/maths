{
  "name": "@ln-markets/maths",
  "version": "1.0.2",
  "description": "LN Markets mathematic formula",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ln-markets/maths.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Victor Afanassieff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ln-markets/maths/issues"
  },
  "homepage": "https://github.com/ln-markets/maths#readme",
  "keywords": [
    "ln-markets",
    "LN Markets",
    "trading",
    "maths"
  ],
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "type": "module",
  "main:": "src/index.js",
  "files": [
    "src/**/*.js"
  ],
  "imports": {
    "#src/*": "./src/*"
  },
  "exports": {
    ".": "./src/index.js"
